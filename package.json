{
  "name": "@amperio/gatsby-plugin-react-redux-providers",
  "version": "1.0.0",
  "description": "Gatsby plugin for React-Redux with built-in server-side rendering support, redux-persist and other providers usability",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "react",
    "redux",
    "react-redux"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "prepublishOnly": "npm run lint",
    "format": "prettier --write {src,examples}/**/*.{js,jsx}",
    "lint": "eslint ./src"
  },
  "author": "Amperio sp. z o.o",
  "license": "MIT",
  "dependencies": {
    "serialize-javascript": "^5.0.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-redux": "*",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby-package": "^0.5.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.13.1",
    "react-redux": "^7.0.3",
    "redux": "^4.0.5"
  },
  "homepage": "https://github.com/amperio-com/gatsby-plugin-react-redux-providers",
  "repository": "https://github.com/amperio-com/gatsby-plugin-react-redux-providers",
  "bugs": {
    "url": "https://github.com/amperio-com/gatsby-plugin-react-redux-providers/issues"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  }
}
